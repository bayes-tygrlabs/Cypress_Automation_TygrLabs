{
  "devDependencies": {
    "cypress": "^15.6.0",
    "mochawesome": "^7.1.4",
    "mochawesome-merge": "^5.0.0",
    "mochawesome-report-generator": "^5.2.0"
  },
  "dependencies": {
    "@faker-js/faker": "^9.5.0"
  },
  "scripts": {
    "test": "npx cypress run",
    "InternalBooking": "npx cypress run --spec 'E:/Automation-scripts-cypress/cypress/e2e/InternalBooking.js' --browser chrome",
    "Regression": "npx cypress run --spec 'cypress/e2e/RegressionScript.js' --browser chrome",
    "ResSync": "npx cypress run --spec 'E:/Automation-scripts-cypress/cypress/e2e/ResSync.js' --browser chrome",
    "merge-reports": "npx mochawesome-merge cypress/reports/*.json > cypress/reports/output.json",
    "generate-report": "npx marge cypress/reports/output.json --reportDir cypress/reports --inline",
    "posttest": "npm run merge-reports && npm run generate-report"
  }
}
