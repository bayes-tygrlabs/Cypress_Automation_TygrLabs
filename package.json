{
  "devDependencies": {
    "cypress": "^15.6.0",
    "mochawesome": "^7.1.4",
    "mochawesome-merge": "^5.0.0",
    "mochawesome-report-generator": "^6.3.2",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "@faker-js/faker": "^9.5.0",
    "express": "^5.1.0"
  },
  "scripts": {
    "clearOldReports": "rm -rf cypress/reports",
    "test": "npx cypress run --spec 'cypress/e2e/InternalBooking.js,cypress/e2e/RegressionScript.js,cypress/e2e/ResSync.js' --browser chrome",
    "mergeAndGenerateReport": "npx mochawesome-merge cypress/reports/json/*.json > cypress/reports/output.json",
    "reportHtmlDirCreation": "mkdir -p cypress/reports/html",
    "mergeReport": "npx marge cypress/reports/output.json -f index -o cypress/reports/html --inline",
    "all": "npm-run-all --continue-on-error clearOldReports test mergeAndGenerateReport reportHtmlDirCreation mergeReport",
    "regression": "npx cypress run --spec 'cypress/e2e/InternalBooking.js'",
    "dispatch": "cypress run --spec 'cypress/e2e/RegressionScript.js'",
    "res-sync-flow": "cypress run --spec 'cypress/e2e/ResSync.js'"

  }
}
